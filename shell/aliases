# Filename: .aliases
# Hold all the aliases I use/find handy.

alias bbd='brew bundle dump --force --describe'
alias bg='batgrep'
alias code='codium'
alias ftrail='<<<${(F)fpath}'
alias gsu='git submodule update --remote --merge'
alias k='clear'
alias ll='eza -laFgh --git --icons --color=always'
alias lla='ls -lhFa'
alias nv='nvim'
alias trail='<<<${(F)path}'
