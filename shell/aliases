# Filename: .aliases
# Hold all the aliases I use/find handy.

alias bbd='brew bundle dump --force --describe'
alias bg='batgrep'
alias code='codium'
alias ftrail='<<<${(F)fpath}'
alias ga='git add'
alias gch='git checkout'
alias gco='git commit -m'
alias gd='git diff'
alias gp='git push'
alias gs='git status'
alias k='clear'
alias ll='eza -laFgh --git --icons --color=always'
alias lla='ls -lhFa'
alias trail='<<<${(F)path}'
alias vim='nvim'
