# Filename: .aliases
# Hold all the aliases I use/find handy.

alias bbd='brew bundle dump --force --describe'
alias bg='batgrep'
alias code='codium'
alias exa='exa -laFh --git'
alias ftrail='<<<${(F)fpath}'
alias gsu='git submodule update --remote --merge'
alias k='clear'
alias ll='ls -lhF'
alias lla='ls -lhFa'
alias ls='exa --git'
alias nv='nvim'
alias trail='<<<${(F)path}'
